<h1>Studios</h1>

<% @studios.each do |studio| %>
    <div id = <%= studio.id %>>
        <h3><%= studio.name %></h3>
        Location: <%= studio.location %>
        <br>
        Movies:
        <% studio.movies.each do |movie|%>
        <ul>
            <li>
            <%= movie.title %>
            </li>
        </ul>
        <% end %>
    </div>
<% end  %>